{
    "manifest_version": 3,
    "name": "Bookmark Butler",
    "version": "1.0.5",
    "permissions": ["bookmarks"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["popup.js"],
        "matches": ["https://code.jquery.com/*"]
      }
    ]
  }
  